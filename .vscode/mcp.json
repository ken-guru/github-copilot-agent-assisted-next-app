{
	"servers": {
		"MCP_DOCKER": {
            "command": "docker",
            "args": [
                "mcp",
                "gateway",
                "run",
                "--verify-signatures",
                "--log-calls",
                "--block-network",
                "--block-secrets",
                "--cpus",
                "1",
                "--memory",
                "1Gb"
            ],
            "type": "stdio"
		}
	},
	"inputs": []
}