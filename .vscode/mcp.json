{
	"servers": {
		"time": {
			"command": "docker",
			"args": [
				"run",
				"--pull=always",
				"-i",
				"--rm",
				"mcp/time"
			],
			"type": "stdio"
		},
		"sequential-thinking": {
			"command": "docker",
			"args": [
				"run",
				"--pull=always",
				"--rm",
				"-i",
				"mcp/sequentialthinking"
			],
			"type": "stdio"
		},
		"memory": {
			"command": "docker",
			"args": [
				"run",
				"--pull=always",
				"-i",
				"-v",
				"claude-memory:/app/dist",
				"--rm",
				"mcp/memory"
			],
			"type": "stdio"
		}
	},
	"inputs": []
}