{
  "name": "github-copilot-agent-assisted-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/update-service-worker-version.js && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cross-browser": "node scripts/run-cross-browser-tests.js",
    "test:browser-compat": "jest --testPathPatterns=cross-browser-compatibility.test.ts",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:headless": "cypress run --headless",
    "cypress:cross-browser": "cypress run --spec 'cypress/e2e/cross-browser-compatibility.cy.ts'",
    "cypress:chrome": "cypress run --browser chrome --spec 'cypress/e2e/cross-browser-compatibility.cy.ts'",
    "cypress:firefox": "cypress run --browser firefox --spec 'cypress/e2e/cross-browser-compatibility.cy.ts'",
    "cypress:edge": "cypress run --browser edge --spec 'cypress/e2e/cross-browser-compatibility.cy.ts'",
    "type-check": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.tests.json --noEmit",
    "clean-build": "node scripts/clean-build.js",
    "clean-build:full": "node scripts/clean-build.js --full",
    "update-sw-version": "node scripts/update-service-worker-version.js",
    "test:visual": "ts-node src/tests/visual-regression/run-visual-tests.ts",
    "test:visual:update": "ts-node src/tests/visual-regression/run-visual-tests.ts -- --update-baselines",
    "test:visual:headed": "ts-node src/tests/visual-regression/run-visual-tests.ts -- --headed",
    "test:visual:suite": "ts-node src/tests/visual-regression/run-visual-tests.ts -- --suite=",
    "test:visual:jest": "jest --config src/tests/visual-regression/jest.visual.config.js",
    "test:qa": "npm run test:visual && npm run cypress:run && npm run test",
    "type-check:ci": "tsc -p tsconfig.ci.json --noEmit",
    "lint:ci": "next lint --config .eslintrc.ci.js",
    "build:ci": "node scripts/update-service-worker-version.js && next build"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^1.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "next": "15.4.6",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/junit-report-builder": "^3.0.2",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "axe-html-reporter": "^2.2.11",
    "axe-playwright": "^2.1.0",
    "cypress": "^14.5.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.5",
    "junit-report-builder": "^5.1.1",
    "make-dir": "^3.1.0",
    "mustache": "^4.2.0",
    "pixelmatch": "^6.0.0",
    "playwright": "^1.55.0",
    "pngjs": "^7.0.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "xmlbuilder": "^15.1.1"
  },
  "overrides": {
    "glob": "^10",
    "test-exclude": "^7.0.1",
    "inflight": "npm:@empty-package/no-inflight@0.0.0"
  }
}